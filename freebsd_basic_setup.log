#!/bin/sh
#
# pretty zfs list
#

zfs list -t snapshot -t filesystem -o name,used,available,exec,devices,checksum,compression,compressratio,quota,usedbysnapshots,mounted,mountpoint \
  | sed \
  -e 's/EXEC/EX/' \
    -e 's/DEVICES/DV/' \
    -e 's/CHECKSUM/CKS/' \
    -e 's/COMPRESS/CMP/' \
    -e 's/USEDSNAP/SNAP/' \
