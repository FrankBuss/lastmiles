

A trivial ray trace experiment with analytic surfaces and 
perhaps some complex math but nothing too adventurous. Yet.

manually hack it together with 






rm -f obs_point.o obs_point ../complex_vector/*.o


$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_check.o ../complex_vector/cplex_check.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_mult.o ../complex_vector/cplex_mult.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_sub.o ../complex_vector/cplex_sub.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_div.o ../complex_vector/cplex_div.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_add.o ../complex_vector/cplex_add.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/check_status.o ../complex_vector/check_status.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_check.o ../complex_vector/cplex_check.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_sqrt.o ../complex_vector/cplex_sqrt.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_sq.o ../complex_vector/cplex_sq.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_theta.o ../complex_vector/cplex_theta.c 
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_mag.o ../complex_vector/cplex_mag.c 
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_add.o ../complex_vector/cplex_vec_add.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_dot.o ../complex_vector/cplex_vec_dot.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_scale.o ../complex_vector/cplex_vec_scale.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_set.o ../complex_vector/cplex_vec_set.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_print.o ../complex_vector/cplex_vec_print.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_vec_copy.o ../complex_vector/cplex_vec_copy.c
$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o ../complex_vector/cplex_quad.o ../complex_vector/cplex_quad.c

$CC $CFLAGS $CPPFLAGS -I ../complex_vector -c -o obs_point.o obs_point.c

$CC $CFLAGS $CPPFLAGS -I ../complex_vector -o obs_point obs_point.o ../complex_vector/cplex_vec_add.o ../complex_vector/cplex_vec_scale.o ../complex_vector/cplex_vec_set.o ../complex_vector/cplex_vec_print.o ../complex_vector/cplex_vec_copy.o ../complex_vector/cplex_mult.o ../complex_vector/cplex_check.o ../complex_vector/cplex_vec_dot.o ../complex_vector/cplex_sub.o ../complex_vector/cplex_quad.o ../complex_vector/cplex_div.o ../complex_vector/check_status.o ../complex_vector/cplex_sqrt.o ../complex_vector/cplex_add.o ../complex_vector/cplex_sq.o ../complex_vector/cplex_theta.o ../complex_vector/cplex_mag.o -lm



